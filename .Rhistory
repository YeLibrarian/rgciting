createCID <- function(data, id = 'R') {
CID <- seq(nrow(data))
CID <- paste(id, formatC(CID, width=3, flag="0"), sep="")
emptyColumns <- sapply(data, function (x) all(is.na(x)))
AID <- NA
data <- cbind(CID, AID, data[!emptyColumns])
}
